type AST tokens: MutableList::Token

constructor AST empty = [
    AST tokens: {}
]

AST append::Token = tokens add: append
AST print = [
    tokens forEach: [
        (it toString) echo
    ]
]

type ASTC commands: MutableList::Command

constructor ASTC empty = [
    ASTC commands: {}
]

ASTC append::Command = commands add: append
ASTC print = [
    commands forEach: [
        (it toString) echo
    ]
]